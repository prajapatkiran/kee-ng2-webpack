{
  "name": "kee-ng2-webpack",
  "version": "0.0.4",
  "description": "An Angular 2 Webpack Starter project",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "build": "webpack --display-error-details",
    "build:aot": "ngc",
    "build:css": "postcss --use autoprefixer dist/*.css -d dist/",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "build:release": "cross-env NODE_ENV=production npm run build",
    "build:sass": "node-sass -o dist/ src/",
    "build:ts": "tsc",
    "check": "npm-check --skip-unused",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "copy:styles": "cpx 'src/**/*.scss' release",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "package": "npm-run-all -s clean:release package:css package:aot build:package package:replace package:minify copy:styles",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:css": "node-sass -o release/ src/themes && node-sass -o release/components src/components",
    "package:minify": "uglifyjs release/index.js --source-map release/index.min.js.map --source-map-url release/index.js.map --compress --mangle --screw-ie8 --output release/index.min.js",
    "package:replace": "node ./config/replace.js",
    "package:ts": "tsc --outDir release",
    "postversion": "git push && git push --tags",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "release": "npm-run-all build:release",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run release",
    "watch": "webpack --display-error-details --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run"
  },
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Kiran Prajapat",
  "homepage": "https://github.com/prajapatkiran/kee-ng2-webpack",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "^2.4.6",
    "@angular/platform-server": "2.4.6",
    "@angular/router": "3.4.6",
    "@angularclass/conventions-loader": "^1.0.2",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "core-js": "^2.4.1",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "jasmine-core": "^2.5.2",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.2",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/common": "~2.4.7",
    "@angular/compiler": "~2.4.7",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "~2.4.7",
    "@angular/platform-browser": "~2.4.7",
    "@angular/platform-browser-dynamic": "~2.4.7",
    "@angular/platform-server": "~2.4.7",
    "@types/jasmine": "2.5.41",
    "@types/node": "^7.0.4",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.4",
    "awesome-typescript-loader": "~3.0.0-beta.10",
    "clean-webpack-plugin": "^0.1.10",
    "codelyzer": "^2.0.1",
    "copy-webpack-plugin": "^4.0.0",
    "core-js": "^2.4.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "fs-extra": "^2.0.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "postcss": "^5.2.11",
    "postcss-loader": "^1.2.2",
    "replace": "^0.3.0",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.5.2",
    "rxjs": "^5.1.0",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^4.3.1",
    "tslint-config-swimlane": "^2.0.1",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "~2.2.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "~2.2.0",
    "webpack-merge": "^2.4.0",
    "webpack-notifier": "^1.5.0",
    "zone.js": "^0.7.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prajapatkiran/kee-ng2-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/prajapatkiran/kee-ng2-webpack/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
